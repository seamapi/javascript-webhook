{
  "name": "@seamapi/webhook",
  "version": "1.1.1",
  "description": "Webhook SDK for the Seam API written in TypeScript.",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/seamapi/javascript-webhook",
  "bugs": "https://github.com/seamapi/javascript-webhook/issues",
  "repository": "seamapi/javascript-webhook",
  "license": "MIT",
  "author": {
    "name": "Seam Labs, Inc.",
    "email": "devops@getseam.com"
  },
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "lib",
    "src",
    "dist",
    "!test",
    "!**/*.test.ts"
  ],
  "scripts": {
    "build": "npm run build:entrypoints",
    "prebuild": "tsx src/index.ts",
    "postbuild": "node ./index.js",
    "build:entrypoints": "npm run build:ts",
    "postbuild:entrypoints": "tsup",
    "build:ts": "tsc --project tsconfig.build.json",
    "prebuild:ts": "del 'index.*' lib",
    "postbuild:ts": "tsc-alias --project tsconfig.build.json",
    "typecheck": "tsc",
    "test": "c8 ava",
    "pretest": "tsx src/index.ts",
    "test:update": "ava --update-snapshots",
    "test:watch": "ava --watch",
    "test:debug": "ava debug --break",
    "lint": "eslint .",
    "postlint": "prettier --check --ignore-path .gitignore .",
    "postversion": "git push --follow-tags",
    "format": "prettier --write --ignore-path .gitignore .",
    "preformat": "eslint --fix .",
    "report": "c8 report"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.1.0"
  },
  "peerDependencies": {
    "@seamapi/types": "^1.166.0"
  },
  "peerDependenciesMeta": {
    "@seamapi/types": {
      "optional": true
    }
  },
  "dependencies": {
    "svix": "^1.15.0"
  },
  "devDependencies": {
    "@seamapi/types": "^1.166.0",
    "@swc/core": "^1.11.29",
    "@types/node": "^22.15.21",
    "ava": "^6.0.1",
    "c8": "^10.1.2",
    "del-cli": "^6.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jiti": "^2.4.2",
    "neostandard": "^0.12.2",
    "prettier": "^3.0.0",
    "tsc-alias": "^1.8.2",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.8.3"
  }
}
